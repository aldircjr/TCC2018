<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="517.0" prefWidth="600.0" style="-fx-border-style: solid;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.FXMLFornecedorController">
   <children>
      <Button fx:id="button_pesquisar" layoutX="490.0" layoutY="14.0" mnemonicParsing="false" onAction="#button_pesquisar" text="Pesquisar">
         <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Icon/pesquisar.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <TextField fx:id="textfield_pesquisa" layoutX="16.0" layoutY="14.0" prefHeight="33.0" prefWidth="471.0" />
      <TableView fx:id="table_fornecedor" layoutX="14.0" layoutY="58.0" prefHeight="383.0" prefWidth="572.0">
        <columns>
          <TableColumn fx:id="column_cnpj" editable="false" prefWidth="155.0" resizable="false" text="CNPJ" />
          <TableColumn fx:id="column_nome" editable="false" prefWidth="269.0" resizable="false" text="Nome" />
            <TableColumn fx:id="column_telefone" editable="false" prefWidth="146.0" resizable="false" text="Telefone" />
        </columns>
