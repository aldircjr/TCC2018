            <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Icon/mais.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="button_editar" contentDisplay="TOP" layoutX="101.0" layoutY="449.0" mnemonicParsing="false" onAction="#button_editar" prefHeight="54.0" prefWidth="85.0" text="Editar">
         <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Icon/editar.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="button_voltar" contentDisplay="TOP" layoutX="501.0" layoutY="449.0" mnemonicParsing="false" onAction="#button_voltar" prefHeight="54.0" prefWidth="85.0" text="Voltar">
