<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="498.0" prefWidth="600.0" style="-fx-border-style: solid;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.FXMLHistoricoMovimentacaoController">
   <children>
      <TableView fx:id="table_historico" layoutX="14.0" layoutY="49.0" prefHeight="393.0" prefWidth="571.0">
        <columns>
          <TableColumn fx:id="colunm_data" prefWidth="130.0" text="Data" />
            <TableColumn fx:id="colunm_usuario" prefWidth="75.0" text="Usuario" />
            <TableColumn fx:id="colunm_movimentacao" prefWidth="75.0" text="Movimentação" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="button_fechar" layoutX="490.0" layoutY="451.0" mnemonicParsing="false" onAction="#button_fechar" prefHeight="33.0" prefWidth="96.0" text="Fechar">
         <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Icon/cancelar.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label layoutX="14.0" layoutY="14.0" text="Produto:">
         <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Icon/produto.png" />
               </image>
            </ImageView>
         </graphic>
      </Label>
      <Label fx:id="label_produto" layoutX="93.0" layoutY="18.0" text="label_produto" />
   </children>
</AnchorPane>
